@model Social
@{
    ViewData["Title"] = "Social";
}

<form method="post">
    <div class="form-group">
        <label asp-for="Icon">Icon</label>
        <input asp-for="Icon" type="text" class="form-control">
    </div>
    <div class="form-group">
        <label asp-for="Link">Link</label>
        <input asp-for="Link" type="text" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>